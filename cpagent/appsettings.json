{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Barbican": {
    "SocketDirectory": "/tmp/cpagent-hermetic",
    "VirtualEnvPath": "./barbican-env",
    "ConfigPath": "./barbican-config",
    "LogPath": "./logs/barbican.log",
    "UseSecureGateway": true,
    "UseNamespaceIsolation": true,
    "VtpmCrypto": {
      "Enabled": true,
      "MasterKeyHandle": "0x81010002",
      "Timeout": 30,
      "PluginNamespace": "barbican_vtpm_crypto.plugin"
    }
  }
}
